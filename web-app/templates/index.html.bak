    <section id="recommendations" ng-controller="rcmCtrl as ctrl">
    	<div class="container">
    		<div class="row">
	        <div class="col-lg-12 text-center">
	            <h2 class="section-heading">Recommendations</h2>
	            <h3 class="section-subheading text-muted">Find something new</h3>
	        </div>
	      </div>
	      
	      <div ng-cloak>
	      	<md-content>
	      		<md-tabs md-dynamic-height md-border-bottom>
	      			<md-tab label="Content-based">
	      				<md-content class="md-padding">
								<div class="row">
					      	<div class="col-md-2"></div>
					    		<div class="col-md-8">
										<div layout="column" ng-cloak>
										{%raw%}
						    		<md-autocomplete flex
						            md-selected-item="ctrl.selectedItem"
						            md-input-minlength="3"
						            md-search-text="ctrl.searchText"
						            md-items="item in ctrl.qrySearch(ctrl.searchText)"
						            md-item-text="item.name"
						            md-delay="300"
						            md-require-match 
						            md-floating-label="Select a game, movie or TV show"
						            md-selected-item-change="ctrl.recommend(item.uniqueID)">
						          <div layout="row" class="item" layout-align="start center">
						          	<div class="row">
						          		<div class="col-md-1" style="text-align: left; width: 50px;">
						          			{{item.itemType}}
						          		</div>
						          		<div class="col-md-1" style="text-align: left; width: 35px;">
						          			<img ng-src="{{item.image}}" class="avatar" width="30" />
						          		</div>
						          		<div class="col-md-8" style="text-align: left; width: 400px;">
						          			<span md-highlight-text="ctrl.searchText">{{item.name}}</span>
						          		</div>
						          		<div class="col-md-2" style="text-align: left; width: 100px;">
						          			<span>{{item.system}}</span>
						          		</div>
						          	</div>
						          </div>
						          <div ng-messages="searchForm.autocompleteField.$error" ng-if="searchForm.autocompleteField.$touched">
						            <div ng-message="md-require-match">Please select an existing item.</div>
						            <div ng-message="minlength">Your entry is not long enough.</div>
						          </div>
						        </md-autocomplete>
						        {%endraw%}
										</div>
					    		</div>
					    		<div class="col-md-2">
								    <md-input-container class="md-icon-float md-icon-right md-block">
								      <input ng-model="recoAmount" type="number" step="1" placeholder="Show" ng-blur="ctrl.recommend(ctrl.selectedItem['uniqueID'])">
								    </md-input-container>
					    		</div>
				    		</div>
				    		
				    		<div class="row">
				    			{%raw%}
					      	<div class="col-md-2"></div>
					    		<div class="col-md-8">
						    		<table>
						    			<thead ng-show="(recoContent | filter:criteria).length">
						    			<tr>
							    			<th style="width: 50px;">
							    				ID
							    			</th>
							    			<th style="width: 435px;" colspan="2">
							    				Name
							    			</th>
							    			<th style="width: 75px;">
							    				Similarity
							    			</th>
							    			<th>
							    				Type
							    			</th>
						    			</tr>
						    			</thead>
						    			<tbody>
						    			<tr ng-repeat="item in recoContent">
							    			<td>
							    				{{item.uniqueID}}
							    			</td>
							    			<td>
							    				<img src="{{item.image}}" title="{{item.name}}" alt="{{item.name}}" width="30" />
							    			</td>
						    				<td>
						    					<a href="http://www.metacritic.com{{item.link}}" target="_blank">{{item.name}}</a>
						    				</td>
							    			<td>
							    				{{item.cossim}}%
							    			</td>
						    				<td>
						    					{{item.itemType}}
						    				</td>
						    			</tr>
						    			</tbody>
						    		</table>
						    	</div>
						    	<div class="col-md-2"></div>
					    		{%endraw%}
				    		</div>


	      				</md-content>
	      			</md-tab>
	      			<md-tab label="Collaborative">
	      				
	      			</md-tab>
	      			<md-tab label="Sentiment">
	      				
	      			</md-tab>
	      		</md-tabs>
	      	</md-content>
	      </div>

    	</div>
    </section>

